<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<typeAliases>
		<typeAlias alias="admin" type="aroundu.model.Admin" />
		<typeAlias alias="seller" type="aroundu.model.Seller" />
		<typeAlias alias="shop" type="aroundu.model.Shop" />
		<typeAlias alias="board" type="aroundu.model.Board" />
		<typeAlias alias="menu" type="aroundu.model.Menu" />
		<typeAlias alias="shopDetail" type="aroundu.model.ShopDetail" />

	</typeAliases>
	
	<mappers>
		<mapper resource="Seller.xml" />
		<mapper resource="Admin.xml" />
		<mapper resource="Shop.xml" />
		<mapper resource="Board.xml" />
		<mapper resource="Menu.xml" />
		<mapper resource="ShopDetail.xml" />
	</mappers>
	
</configuration>