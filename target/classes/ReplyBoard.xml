<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="replyBoardns">
	<!-- Use type aliases to avoid typing the full classname every time. -->
	<resultMap id="replyBoardResult" 		type="replyBoard">
		<result property="rno" 		column="bd_num" />
		<result property="replytext" 	column="bd_subject" />
		<result property="replyer"	column="bd_content" /> 
		<result property="regdate" 	column="bd_regDate" />
		
	</resultMap>

	
	
		
</mapper>
	