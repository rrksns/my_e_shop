	<typeAliases>
		<!-- <typeAlias alias="admin" type="aroundu.model.Admin" /> -->
		<typeAlias alias="seller" type="aroundu.model.Seller" />
		<typeAlias alias="shop" type="aroundu.model.Shop" />
